.sign-up-completion-page
  %header.sign-up-completion-page__header
    = link_to image_tag('//www-mercari-jp.akamaized.net/assets/img/common/common/logo.svg?3761641567', width: '185px', height: '49px' ), '/'
    .sign-up-completion-page__header__process-bar
      %ol.sign-up-completion-page__header__process-bar__inner
        %li.text1
          会員情報
          .dot1
          %hr.line1
        %li.text2
          電話番号認証
          .dot2
          %hr.line2
        %li.text3
          お届け先住所入力
          .dot3
          %hr.line3
        %li.text4
          支払い方法
          .dot4
          %hr.line4
        %li.text5
          完了
          .dot5
  -# %main.sign-up-completion-page__main
  -#   %h2.sign-up-completion-page__title
  -#     会員登録完了
  = render 'shared/app-banner'
  = render 'shared/exhibition-page-footer'
  